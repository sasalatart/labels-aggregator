{
  "extends": ["airbnb-base", "plugin:prettier/recommended"],
  "plugins": ["googleappsscript", "prettier"],
  "env": {
    "googleappsscript/googleappsscript": true
  },
  "rules": {
    "prettier/prettier": "error"
  },
  "globals": {
    "assertHeaders": "readonly",
    "assertInputSheet": "readonly",
    "compareText": "readonly",
    "display": "readonly",
    "HeadersNotFoundError": "readonly",
    "InputSheetNotFoundError": "readonly",
    "normalizeCode": "readonly",
    "normalizePrice": "readonly"
  }
}
